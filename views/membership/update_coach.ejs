    if (result1 && !result2) {

      alert("請回答後面的問題。")
    }
    if (result2 && !result1) {
      alert("請勾選複選框。")
    }

    if ((result3 && !result4)) {
      alert("請回答後面的問題。")
    }
    if ((result4 && !result3)) {
      alert("請勾選複選框。")
    }

    if (((result1 && result2) > 0)  &&  ((result3 && result4 )> 0) || ((!result1 && !result2) > 0)  &&  ((result3 && result4 )> 0) || ((result1 && result2) > 0)  &&  ((!result3 && !result4 )> 0) ) {
      return true;
    } else {
      return false;

    }

  }
  </script>