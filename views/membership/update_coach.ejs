        <label style="font-size:13px">姓名 (中文)
          Name (Chi)</label>
        <input type="text" name="Coach[ChiName]" value="<%= coach.ChiName%>"
          onkeyup="value=value.replace(/[^\u4E00-\u9FA5]/g,'')">
      </div>
      <br>
      <div class="form-group">
        <label style="font-size:13px"> 香港身份證號碼
          HKID no.
        </label>
        <input type="text" name="Coach[Id]" value="<%= coach.Id%>" onkeyup="value=value.replace(/[^\a-\z\A-\Z0-9]/g,'')"
       >

        
        <label style="font-size:13px"> 性別
          Gender</label>
  
        <% if (coach.Gender=="女"){ %>
        <input type="radio" name="Coach[Gender]" value="女" checked>
        <label style="font-size:14px">女 Female</label>
        <input type="radio" name="Coach[Gender]" value="男">
        <label style="font-size:14px">男 Male</label>
  
