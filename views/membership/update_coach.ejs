        <input type="radio" name="Coach[Gender]" value="女">
        <label style="font-size:14px">女 Female</label>
        <input type="radio" name="Coach[Gender]" value="男" checked>
        <label style="font-size:14px">男 Male</label>
  
        <%} else {%>
        <input type="radio" name="Coach[Gender]" value="女" required>
        <label style="font-size:14px">女 Female</label>
        <input type="radio" name="Coach[Gender]" value="男" required>
        <label style="font-size:14px">男 Male</label>
        <%}%> 
      </div>
      <br>
