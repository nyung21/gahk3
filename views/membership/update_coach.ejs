      </tr>
    </table>
  
  
    <form action="/coach/update_coach/<%=coach.id%>" method="POST">
      <div class="form-group">
        <br>
          <label style="font-size:14px"><b>支票號碼</b></label>
  
          <input type="text" name="Coach[CheckNo]"  onkeyup="value=value.replace(/[^0-9]/g,'')" value="<%=coach.CheckNo%>">
        
      </div>

      <div class="form-group">
        <label style="font-size:14px"><b>確認教練?</b></label>
  
