          核準教練
          Accredited Coach
          <input type="radio" name="Coach[Level]" value="第一級" checked>
          第一級
          Level 1
          <input type="radio" name="Coach[Level]" value="第二級">
          第二級
          Level 2
          <input type="radio" name="Coach[Level]" value="第三級">
          第三級
          Level 3
  
  
          <% }else if (coach.Level=="第二級"){%>
  
          <input type="radio" name="Coach[Level]" value="核準教練">
          核準教練
          Accredited Coach
          <input type="radio" name="Coach[Level]" value="第一級">
          第一級
          Level 1
          <input type="radio" name="Coach[Level]" value="第二級" checked>
          第二級
          Level 2
          <input type="radio" name="Coach[Level]" value="第三級">
          第三級
          Level 3
          <% }else if (coach.Level=="第三級"){%>
  
          <input type="radio" name="Coach[Level]" value="核準教練">
          核準教練
          Accredited Coach
          <input type="radio" name="Coach[Level]" value="第一級">
          第一級
          Level 1
          <input type="radio" name="Coach[Level]" value="第二級">
          第二級
          Level 2
          <input type="radio" name="Coach[Level]" value="第三級" checked>
          第三級
          Level 3
  
          <%} else {%>
          <input type="radio" name="Coach[Level]" value="核準教練" required>
          核準教練
          Accredited Coach
