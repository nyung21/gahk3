          第一級
          Level 1
          <input type="radio" name="Coach[Level]" value="第二級">
          第二級
          Level 2
          <input type="radio" name="Coach[Level]" value="第三級">
          第三級
          Level 3
  
  
          <% }else if (coach.Level=="第一級"){%>
  
