        <input type="text" name="Coach[Have2]" value="<%= coach.Have2%>" id="coachH_other_if" required>
  
  
         <% }else{ %>%>
  
          <input type="checkbox" name="Coach[HaveBeenCoach1]" value="選擇" id="coachH_other">
          <label style="font-size:10px"> 其他有關體操教學經驗，請列明：
            Other coaching experience related to gymnastics, please give details:
    
          </label>
    
          <input type="text" name="Coach[Have2]" value="<%= coach.Have2%>" id="coachH_other_if">
          <%}%>
  
      
      </div>
  
      <br>
  
  
